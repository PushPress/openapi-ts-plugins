{
  "name": "@pushpress/openapi-ts-plugins",
  "version": "0.1.3",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "import": "./dist/tools/index.js",
      "require": "./dist/tools/index.cjs"
    },
    "./neverthrow": {
      "types": "./dist/neverthrow/index.d.ts",
      "import": "./dist/neverthrow/index.js",
      "require": "./dist/neverthrow/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest run",
    "gen": "openapi-ts",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@hey-api/openapi-ts": "^0.80.1",
    "@rslib/core": "^0.11.0",
    "@types/node": "^22.16.5",
    "eslint": "^9.30.0",
    "globals": "^16.2.0",
    "msw": "^2.0.0",
    "neverthrow": "^8.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "zod": "^4.0.13"
  }
}
